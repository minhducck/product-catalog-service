{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "esModuleInterop": true,
    "webpack": false,
    "tsConfigPath": "apps/backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/backend",
  "projects": {
    "attribute": {
      "type": "application",
      "root": "apps/attribute",
      "entryFile": "main",
      "sourceRoot": "apps/attribute/src",
      "compilerOptions": {
        "tsConfigPath": "apps/attribute/tsconfig.app.json"
      }
    },
    "attribute-serverless": {
      "type": "application",
      "root": "apps/attribute",
      "entryFile": "serverless",
      "sourceRoot": "apps/attribute/src",
      "compilerOptions": {
        "tsConfigPath": "apps/attribute/tsconfig.app.json"
      }
    },
    "backend": {
      "type": "application",
      "root": "apps/backend",
      "entryFile": "main",
      "sourceRoot": "apps/backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend/tsconfig.app.json"
      }
    },
    "category": {
      "type": "application",
      "root": "apps/category",
      "entryFile": "main",
      "sourceRoot": "apps/category/src",
      "compilerOptions": {
        "tsConfigPath": "apps/category/tsconfig.app.json"
      }
    },
    "category-attribute-index": {
      "type": "application",
      "root": "apps/category-attribute-index",
      "entryFile": "main",
      "sourceRoot": "apps/category-attribute-index/src",
      "compilerOptions": {
        "tsConfigPath": "apps/category-attribute-index/tsconfig.app.json"
      }
    },
    "category-serverless": {
      "type": "application",
      "root": "apps/category",
      "entryFile": "serverless",
      "sourceRoot": "apps/category/src",
      "compilerOptions": {
        "tsConfigPath": "apps/category/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "mysql-database": {
      "type": "library",
      "root": "libs/mysql-database",
      "entryFile": "index",
      "sourceRoot": "libs/mysql-database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mysql-database/tsconfig.lib.json"
      }
    },
    "product": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "main",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json"
      }
    },
    "product-serverless": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "serverless",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json"
      }
    },
    "performance-fixtures": {
      "type": "application",
      "root": "apps/performance-fixtures",
      "entryFile": "main",
      "sourceRoot": "apps/performance-fixtures/src",
      "compilerOptions": {
        "tsConfigPath": "apps/performance-fixtures/tsconfig.app.json"
      }
    }
  }
}